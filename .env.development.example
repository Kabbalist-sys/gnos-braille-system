# Development Environment Variables
# Copy this file to .env.development and fill in your development values

# Firebase Development Configuration
FIREBASE_API_KEY_DEV=your_development_api_key_here
FIREBASE_AUTH_DOMAIN_DEV=your-project-dev.firebaseapp.com
FIREBASE_PROJECT_ID_DEV=your-project-dev
FIREBASE_STORAGE_BUCKET_DEV=your-project-dev.appspot.com
FIREBASE_MESSAGING_SENDER_ID_DEV=123456789
FIREBASE_APP_ID_DEV=1:123456789:web:abcdef123456
FIREBASE_MEASUREMENT_ID_DEV=G-ABCDEF1234

# API Configuration
BRAILLE_API_URL_DEV=http://localhost:5000
API_TIMEOUT_DEV=10000

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASHLYTICS=false
ENABLE_PERFORMANCE_MONITORING=false
ENABLE_DEBUG_LOGGING=true

# Security Settings (more relaxed for development)
MAX_TRANSLATION_LENGTH=50000
MAX_EXPORT_SIZE=100000000
SESSION_TIMEOUT=7200000

# Rate Limiting (more generous for development)
MAX_REQUESTS_PER_MINUTE=1000
MAX_TRANSLATIONS_PER_HOUR=10000

# Cache Settings
CACHE_TTL=60000
MAX_CACHE_SIZE=50000000
