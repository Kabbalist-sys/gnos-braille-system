# Production Environment Variables
# Copy this file to .env.production and fill in your production values

# Firebase Production Configuration
FIREBASE_API_KEY_PROD=your_production_api_key_here
FIREBASE_AUTH_DOMAIN_PROD=your-project-prod.firebaseapp.com
FIREBASE_PROJECT_ID_PROD=your-project-prod
FIREBASE_STORAGE_BUCKET_PROD=your-project-prod.appspot.com
FIREBASE_MESSAGING_SENDER_ID_PROD=123456789
FIREBASE_APP_ID_PROD=1:123456789:web:abcdef123456
FIREBASE_MEASUREMENT_ID_PROD=G-ABCDEF1234

# API Configuration
BRAILLE_API_URL_PROD=https://api.gnos-braille.com
API_TIMEOUT_PROD=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_DEBUG_LOGGING=false

# Security Settings
MAX_TRANSLATION_LENGTH=10000
MAX_EXPORT_SIZE=50000000
SESSION_TIMEOUT=3600000

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=100
MAX_TRANSLATIONS_PER_HOUR=1000

# Cache Settings
CACHE_TTL=300000
MAX_CACHE_SIZE=100000000
